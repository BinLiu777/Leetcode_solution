class Solution {
    public boolean isAnagram(String s, String t) {
        char[] s_chars = s.toCharArray();
        char[] t_chars = t.toCharArray();
        Arrays.sort(s_chars);
        Arrays.sort(t_chars);
        //System.out.println(String.valueOf(s_chars));
        return String.valueOf(s_chars).equals(String.valueOf(t_chars));
    }
}
